<div class="letter-grid">
        
    <% letterArray.forEach(letter => { %>
        <div class="letter-page-card">
            <img src="/images/letters/<%= letter %>.png" alt="" style="height: 100px;">
            <video width="200" height="200" controls>
            <source src="/videos/<%= letter %>.mp4" type="">
          </video>
        </div>
    <% }); %>

</div>

